{
  "go.useLanguageServer": true,
  "workbench.statusBar.visible": true,
  "breadcrumbs.enabled": false,
  "window.menuBarVisibility": "toggle",
  "editor.multiCursorModifier": "ctrlCmd",
  "files.watcherExclude": {
    "**/.git/objects/**": true,
    "**/.git/subtree-cache/**": true,
    "**/.hg/store/**": true,
    "**/node_modules/**": true,
  },
  "vim.useSystemClipboard": true,
  "vim.highlightedyank.enable": true,
  "vim.normalModeKeyBindingsNonRecursive": [
    {
        "before": ["<C-o>"],
        "commands": ["workbench.action.navigateBack"]
    },
    {
        "before": ["<C-i>"],
        "commands": ["workbench.action.navigateForward"]
    },
    {
      "before": ["<leader>", "j"],
      "commands": [":tabprevious"]
    },
    {
      "before": ["<leader>", "b"],
      "commands": ["workbench.view.explorer"]
    },
    {
      "before": ["<leader>", "k"],
      "commands": [":tabnext"]
    },
    {
      "before": ["<C-q>"],
      "commands": [":q"]
    },
    {
      "before": ["<C-x>"],
      "commands": ["workbench.action.closeActiveEditor"]
    },
    {
      "before": ["K"],
      "commands": ["editor.action.showHover"]
    },
    {
      "before": ["g", "d"],
      "commands": ["editor.action.revealDefinition"]
    },
    {
      "before": ["g", "r"],
      "commands": ["editor.action.goToReferences"]
    },
    {
      "before": ["g", "I"],
      "commands": ["editor.action.goToImplementation"]
    },
    {
      "before": ["g", "D"],
      "commands": ["editor.action.revealDeclaration"]
    },
    {
      "before": ["<leader>", "D"],
      "commands": ["editor.action.goToTypeDefinition"]
    },
    {
      "before": ["<leader>", "c", "s"],
      "commands": ["workbench.action.gotoSymbol"]
    },
    {
      "before": ["<leader>", "d", "c"],
      "commands": ["workbench.action.debug.start"]
    },
    {
      "before": ["<leader>", "d", "b"],
      "commands": ["editor.debug.action.toggleBreakpoint"]
    },
    {
      "before": ["<leader>", "w", "s"],
      "commands": ["workbench.action.showAllSymbols"]
    },
    {
      "before": ["<leader>", "r", "n"],
      "commands": ["editor.action.rename"]
    },
    {
      "before": ["<leader>", "c", "a"],
      "commands": ["editor.action.quickFix"]
    },
    {
      "before": ["<leader>", "e", "e"],
      "commands": ["workbench.view.explorer"]
    },
    {
      "before": ["<leader>", "s", "b"],
      "commands": ["workbench.action.showAllEditors"]
    },
    {
      "before": ["<leader>", "s", "f"],
      "commands": ["workbench.action.quickOpen"]
    },
    {
      "before": ["<leader>", "s", "g"],
      "commands": [
        {
          "command": "periscope.search",
          "when": "editorTextFocus"
        }
      ]
    },
    {
      "before": ["<leader>", "g", "g"],
      "commands": ["lazygit-vscode.toggle"]
    },
    {
      "before": ["<leader>", "c", "i"],
      "commands": ["workbench.panel.chat"]
    },
    {
      "before": ["<leader>", "c", "f"],
      "commands": ["editor.action.formatDocument"]
    },
    {
        "before": ["<C-b>", "z"],
        "commands": ["workbench.action.toggleMaximizeEditorGroup"]
    },
    {
      "before": ["<C-w>", "L"],
      "commands": ["workbench.action.moveEditorToNextGroup"]
    },
    {
      "before": ["<C-w>", "H"],
      "commands": ["workbench.action.moveEditorToPreviousGroup"]
    },
    {
      "before": ["<leader>", "<leader>"],
      "commands": ["vscode-harpoon.editorQuickPick"]
    },
    {
      "before": ["<leader>", "h", "a"],
      "commands": ["vscode-harpoon.addEditor"]
    },
    {
      "before": ["<leader>", "h", "h"],
      "commands": ["vscode-harpoon.editEditors"]
    },
    {
      "before": ["<leader>", "h", "1"],
      "commands": ["vscode-harpoon.gotoEditor1"]
    },
    {
      "before": ["<leader>", "h", "2"],
      "commands": ["vscode-harpoon.gotoEditor2"]
    },
    {
      "before": ["<leader>", "h", "3"],
      "commands": ["vscode-harpoon.gotoEditor3"]
    },
    {
      "before": ["<leader>", "h", "4"],
      "commands": ["vscode-harpoon.gotoEditor4"]
    },
    {
      "before": ["<leader>", "h", "5"],
      "commands": ["vscode-harpoon.gotoEditor5"]
    },
    {
      "before": ["<leader>", "c", "c"],
      "commands": ["editor.action.commentLine"]
    },
  ],
  "vim.insertModeKeyBindings": [
    {
      "before": ["<C-k>"],
      "commands": ["editor.action.triggerParameterHints"]
    }
  ],
  "vim.visualModeKeyBindingsNonRecursive": [
    {
        "before": ["<C-o>"],
        "commands": ["workbench.action.navigateBack"]
    },
    {
        "before": ["<C-i>"],
        "commands": ["workbench.action.navigateForward"]
    },
    {
      "before": ["<leader>", "c", "c"],
      "commands": ["editor.action.commentLine"]
    },
    {
      "before": ["<leader>", "c", "f"],
      "commands": ["editor.action.formatSelection"]
    }
  ],
  "vim.handleKeys": {
    "<C-a>": false,
    "<C-f>": false,
    "<C-b>": false
  },
  "vim.leader": "<space>",
  "window.zoomLevel": 0,
  "editor.fontFamily": "'Inconsolata LGC Nerd Font Mono','MonaspiceNe Nerd Font','JetBrainsMono Nerd Font'",
  "eslint.validate": [
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact"
  ],
  "C_Cpp.updateChannel": "Insiders",
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": "explicit"
  },
  "cmake.configureOnOpen": true,
  "vscode-neovim.neovimExecutablePaths.linux": "/usr/bin/nvim",
  "editor.fontLigatures": true,
  "workbench.iconTheme": "material-icon-theme",
  "javascript.suggest.autoImports": false,
  "typescript.suggest.autoImports": false,
  "debug.console.fontSize": 12,
  "markdown.preview.fontSize": 12,
  "terminal.integrated.fontSize": 12,
  "editor.suggestSelection": "first",
  "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
  "go.toolsManagement.autoUpdate": true,
  "files.exclude": {
    "**/.classpath": true,
    "**/.project": true,
    "**/.settings": true,
    "**/.factorypath": true
  },
  "vscode-neovim.neovimExecutablePaths.darwin": "/opt/homebrew/bin/nvim",
  "extensions.experimental.affinity": {
    "asvetliakov.vscode-neovim": 1
  },
  "workbench.colorTheme": "Tokyo Night",
  "editor.minimap.enabled": false,
  "vim.smartRelativeLine": true,
  "vim.foldfix": true,
  "vim.neovimPath": "/opt/homebrew/bin/nvim",
  "vim.neovimUseConfigFile": false,
  "vim.enableNeovim": true,
  "vim.neovimConfigPath": "/Users/s0h0oz1/.config/kickstarter/init.lua",
  "C_Cpp.default.compilerPath": "/opt/homebrew/opt/llvm/bin/clang++",
  "github.copilot.enable": {
    "*": false,
    "plaintext": false,
    "markdown": false,
    "scminput": false
  },
}
